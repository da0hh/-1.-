#include <iostream>

  int main() {
  int a = 42;
  int b = 153;
  std::cout << "a: " << a << "\n"; /* На экран будет выведено 42 */
  std::cout << "b: " << b << "\n"; /* На экран будет выведено 153 */

/* Меняем значения */
  a = b;
  b = a/3-9;
  std::cout << "a: " << a << "\n"; /* На экран будет выведено 153 */
  std::cout << "b: " << b << "\n"; /* На экран будет выведено 42 */
}
